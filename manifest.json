{
  "name": "Github Card Printer",
  "version": "1.0",
  "description": "Print Agile cards from Zenhub-Github issues",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "scripts/content.js"
      ]
    }
  ],
  "manifest_version": 2
}